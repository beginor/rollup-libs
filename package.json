    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "history": "^5.3.0",
    "react-router": "^6.5.0",
    "react-router-dom": "^6.5.0",
    "react-bootstrap": "^2.7.0",
    "antd": "^4.24.5",
    "@ant-design/icons-svg": "^4.2.1",
    "mapbox-gl": "^2.11.1",
    "@turf/turf": "^6.5.0",
    "rxjs": "^7.8.0",
