    "react-dom": "^18.2.0",
    "@types/react-dom": "^18.2.7",
    "history": "^5.3.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-bootstrap": "^2.8.0",
    "antd": "^5.7.0",
